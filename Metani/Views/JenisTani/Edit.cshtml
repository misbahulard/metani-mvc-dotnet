@model Metani.Models.JenisTani

@{
    ViewBag.Title = "Update";
}
<h2>Edit</h2>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="form-horizontal">
        <h4>Movie</h4>
        <hr />

        <input type="text" placeholder="id" name="idJenisTani" value="@Model.IdJenisTani" />

        <div class="form-group">
            <label class="control-label col-md-2">Nama Jenis Tani : </label>
            <div class="col-md-10">
                <input type="text" placeholder="Masukkan jenis tani" id="namaJenisTani" name="namaJenisTani" value="@Model.NamaJenisTani">
            </div>
        </div>

        @*Genre and Price removed for brevity.*@
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </div>
    </div>
}
<div>
    @Html.ActionLink("Back to List", "Index")
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}