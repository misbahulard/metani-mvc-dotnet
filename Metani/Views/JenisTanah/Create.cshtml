@model Metani.Models.JenisTanah

@{
    ViewBag.Title = "Create Jenis Tanah";
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="main-content">
        <section class="section">
            <div class="section-header">
                <h2>@ViewBag.Title</h2>
            </div>
            <div class="section-body">
                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-header">
                                <h4>Data Jenis Tanah</h4>
                            </div>
                            <div class="card-body">
                                <div class="form-group row mb-4">
                                    <label class="col-form-label text-md-right col-12 col-md-3 col-lg-3">Jenis Tanah</label>
                                    <div class="col-sm-12 col-md-7">
                                        <input type="text" class="form-control" placeholder="Masukkan Jenis Tanah" id="namaJenisTanah" name="namaJenisTanah">

                                    </div>
                                </div>
                                <div class="form-group row mb-4">
                                    <label class="col-form-label text-md-right col-12 col-md-3 col-lg-3"></label>
                                    <div class="col-sm-12 col-md-7">
                                        <input type="submit" value="Create" class="btn btn-primary" />
                                        <a class="btn btn-info" href="~/JenisTanah/Index">Back to List</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}