@model IEnumerable<Metani.Models.JenisTanah>

@{
    ViewBag.Title = "Jenis Tanah";
}
<h2>@ViewBag.Title.</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            ID Jenis Tanah
        </th>
        <th>
            Nama Tanah
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.IdJenisTanah
            </td>
            <td>
                @item.NamaJenisTanah
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.IdJenisTanah }) |
                @Html.ActionLink("Details", "Details", new { id = item.IdJenisTanah }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.IdJenisTanah })
            </td>
        </tr>
    }

</table>
