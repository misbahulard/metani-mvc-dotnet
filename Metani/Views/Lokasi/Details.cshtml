@model Metani.Models.Lokasi

@{
    ViewBag.Title = "Lokasi Tani";
}
<h2>@ViewBag.Title.</h2>

<table class="table">
    <tr>
        <th>
            ID Lokasi Tani
        </th>
        <th>
            Kecamatan
        </th>
        <th>
            Kabupaten
        </th>
        <th>
            Provinsi
        </th>
        <th>
            Kodepos
        </th>
        <th>
            Latitude
        </th>
        <th>
            Longitude
        </th>
        <th></th>
    </tr>
    <tr>
        <td>
            @Model.IdLokasi
        </td>
        <td>
            @Model.Kecamatan
        </td>
        <td>
            @Model.Kabupaten
        </td>
        <td>
            @Model.Provinsi
        </td>
        <td>
            @Model.KodePos
        </td>
        <td>
            @Model.Latitude
        </td>
        <td>
            @Model.Longitude
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = Model.IdLokasi }) |
            @Html.ActionLink("Delete", "Delete", new { id = Model.IdLokasi })
        </td>
    </tr>

</table>
