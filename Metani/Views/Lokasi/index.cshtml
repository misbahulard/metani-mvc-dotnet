@model IEnumerable<Metani.Models.Lokasi>

@{
    ViewBag.Title = "Lokasi";
}
<h2>@ViewBag.Title.</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            ID Lokasi
        </th>
        <th>
            Kecamatan
        </th>
        <th>
            Kabupaten
        </th>
        <th>
            Provinsi
        </th>
        <th>
            Kode Pos
        </th>
        <th>
            Latitude
        </th>
        <th>
            Longitude
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @item.IdLokasi
        </td>
        <td>
            @item.Kecamatan
        </td>
        <td>
            @item.Kabupaten
        </td>
        <td>
            @item.Provinsi
        </td>
        <td>
            @item.KodePos
        </td>
        <td>
            @item.Latitude
        </td>
        <td>
            @item.Longitude
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.IdLokasi }) |
            @Html.ActionLink("Details", "Details", new { id = item.IdLokasi }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.IdLokasi })
        </td>
    </tr>
    }

</table>
